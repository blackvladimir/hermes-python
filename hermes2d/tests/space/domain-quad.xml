<?xml version="1.0" encoding="utf-8"?>
<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../xml_schemas/mesh_h2d_xml.xsd">

  <vertices>
    <vertex x="0.0" y="0.0" i="0"/>
    <vertex x="1.0" y="0.0" i="1"/>
    <vertex x="1.0" y="1.0" i="2"/>
    <vertex x="0.0" y="1.0" i="3"/>
  </vertices>

  <elements>
    <element:quad v1="0" v2="1" v3="2" v4="3" marker="Copper" />
  </elements>

  <edges>
    <edge v1="0" v2="1" marker="Bottom" />
    <edge v1="1" v2="2" marker="Right" />
    <edge v1="2" v2="3" marker="Top" />
    <edge v1="3" v2="0" marker="Left" />
  </edges>
</mesh:mesh>
